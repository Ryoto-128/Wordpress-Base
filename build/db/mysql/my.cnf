# mysqld setting file 

# mysql_section: mysql_server setting
[mysqld]
character-set-server=utf8mb4
collation-server=utf8mb4_general_ci
# port=3306
# socket=/tmp/mysql.sock
# key_buffer_size=16M
# max_allowed_packet=8M


# client_section: client_tools setting
[client]
default-character-set=utf8mb4
# port=3306
# socket=/tmp/mysql.sock

